{
  "framework": null,
  "buildCommand": null,
  "outputDirectory": ".",
  "rewrites": [
    { "source": "/toeic-words", "destination": "/" },
    { "source": "/toeic-words/", "destination": "/" },
    { "source": "/toeic-words/:path*", "destination": "/:path*" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    }
  ]
}
